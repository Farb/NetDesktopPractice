<Window x:Class="WpfAppNetTest.Views.Template.PreDataTemplate"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:control="clr-namespace:WpfAppNetTest.Control"
        mc:Ignorable="d"
        Title="PreDataTemplate" Height="450" Width="601">
    <StackPanel Orientation="Horizontal" Margin="5,0">
        <control:CarDetailView x:Name="carDetailView"/>
        <ListBox x:Name="listBoxCars" Width="180" Margin="5,0" SelectionChanged="listBoxCars_SelectionChanged"/>
    </StackPanel>
</Window>
